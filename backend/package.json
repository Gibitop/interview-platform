    "type": "module",
    "scripts": {
        "dev": "tsx --watch src/main.ts",
        "check-types": "tsc --noEmit",
        "build": "rm -rf ./dist/* && swc ./src/* -d ./dist --config-file .swcrc && fix-esm-import-path ./dist",
