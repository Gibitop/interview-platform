        "start": "node dist/src/main.js"
    },
    "devDependencies": {
        "@swc/cli": "^0.3.12",
        "@swc/core": "^1.5.24",
        "@types/node": "^20.12.7",
        "@types/ws": "^8.5.10",
        "fix-esm-import-path": "^1.6.0",
        "tsx": "^4.11.0",
        "typescript": "^5.4.5"
    },
    "dependencies": {
        "@fastify/websocket": "10.0.1",
        "axios": "^1.6.8",
        "chokidar": "^3.6.0",
        "fastify": "^4.27.0",
        "fastify-socket.io": "^5.0.0",
        "fastify-type-provider-zod": "^1.1.9",
        "json-joy": "^16.5.0",
        "node-pty": "^1.0.0",
        "socket.io": "^4.7.5",
        "zod": "^3.23.8"
    }
